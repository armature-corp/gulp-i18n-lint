{
  "name": "gulp-i18n-lint",
  "version": "0.0.1",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "nyc": "^10.1.2",
    "tap-nyan": "^1.1.0",
    "vinyl": "^2.0.1"
  },
  "dependencies": {
    "chalk": "^1.0.0",
    "htmlparser2": "^3.0.0",
    "lodash": "^3.0.0",
    "through2": "^2.0.3"
  },
  "scripts": {
    "test": "ava",
    "coverage": "nyc ava --tap | tap-nyan",
    "report": "nyc report --reporter=html"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "ava": {
      "babel": {
        "presets": [
          "es2015"
        ]
      }
    }
  }
}
